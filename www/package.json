{
  "name": "react-expressjs",
  "version": "1.0.0",
  "description": "Boilerplate for ReactJS project with ExpressJS server",
  "main": "src/index.js",
  "scripts": {    
    "clean": "rimraf dist/",
    "build:css": "node-sass-chokidar --include-path ./node_modules ./src/ -o ./src/",
    "build:bundle": "webpack --mode development --config ./src/webpack.config.js",
    "build": "env-cmd development npm-run-all clean build:bundle build:css",    
    "watch:server": "nodemon --watch ./src/**/* --exec babel-node --presets @babel/preset-env ./react-server/"
  },
  "dependencies": {
    "@babel/node": "^7.2.2",
    "@babel/polyfill": "^7.2.5",
    "cross-env": "^5.0.5",
    "env-cmd": "^8.0.2",
    "history": "^4.7.2",
    "immutable": "^3.8.2",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "prop-types": "^15.5.10",
    "rc-time-picker": "^3.4.0",
    "react": "^16.5.2",
    "react-datepicker": "^2.0.0",
    "react-dom": "^16.5.2",
    "react-hot-loader": "^3.1.3",
    "react-loadable": "^5.5.0",
    "react-redux": "^5.1.1",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.1.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-toolbox": "^2.0.0-beta.13",
    "redux": "^4.0.1",
    "redux-form": "^7.4.2",
    "redux-immutable": "^4.0.0",
    "redux-immutablejs": "0.0.8",
    "redux-localstorage": "^0.4.1",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "save-dev": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-es2017": "^7.0.0-beta.53",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "axios": "^0.18.0",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-inline-import": "^3.0.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2017": "^6.24.1",
    "babel-register": "^6.26.0",
    "eslint": "^5.6.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "node-sass-chokidar": "^1.3.4",
    "nodemon": "^1.18.9",
    "react-scripts": "^2.1.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.19.1",
    "webpack-cli": "^3.2.0",
    "webpack-hot-middleware": "^2.24.3"
  },
  "bundledDependencies": [
    "cross-env",
    "ejs",
    "express",
    "prop-types",
    "react",
    "react-dom",
    "react-redux",
    "react-router",
    "react-router-dom",
    "redux"
  ],
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}